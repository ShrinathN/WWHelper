{
    "manifest_version": 3,
    "name": "Data Passing Extension",
    "version": "1.0",
    "permissions": [
        "storage",
        "scripting"
    ],
    "host_permissions": [
        "https://web.whatsapp.com/",
        "https://web.whatsapp.com/*",
        "http://127.0.0.1/*"
    ],
    "background": {
        "service_worker": "js/background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "https://web.whatsapp.com/"
            ],
            "js": [
                "js/content.js"
            ]
        }
    ]
}